#include <stdio.h>

int main()
{
    for(int i = 1; i <= 100; ++i){
        if( i % 15 == 0){
            printf("%dFizzBuzz\n", i);
        }else if (i % 5 == 0){
            printf("%dBuzz\n", i);
        }else if (i % 3 == 0){
            printf("%dFizz\n", i);
        }else printf("%d\n", i); 
    }
    return 0;
}
